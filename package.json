{
  "name": "blank-theme",
  "version": "1.0.0",
  "description": "A boilerplate from Wordpress theme.",
  "main": "tailwind.config.js",
  "scripts": {
    "dev": "npx tailwindcss -i ./tailwind.css -o ./style.css",
    "watch": "npx tailwindcss -i ./tailwind.css -o ./style.css --watch",
    "build": "NODE_ENV=production npx tailwindcss -i ./tailwind.css -o ./style.css --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devnicholas/blank-theme.git"
  },
  "keywords": [
    "Wordpress",
    "Theme",
    "Custom"
  ],
  "author": "Nicholas Stefano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devnicholas/blank-theme/issues"
  },
  "homepage": "https://github.com/devnicholas/blank-theme#readme",
  "dependencies": {
    "tailwindcss": "^3.0.23"
  }
}
